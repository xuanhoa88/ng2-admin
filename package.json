    "tslint": "~3.15.1",
    "typescript": "~2.0.3",
    "uglify-js": "^2.7.5",
    "url-loader": "^0.5.7",
    "webpack": "~2.1.0-beta.22",
    "webpack-dev-server": "~2.1.0-beta.4",
